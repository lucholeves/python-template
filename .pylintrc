[MESSAGES CONTROL]

# Enable the message, report, category or checker with the given id(s). You can
# either give multiple identifier separated by comma (,) or put this option
# multiple time.
#enable=

# Disable the message, report, category or checker with the given id(s). You
# can either give multiple identifier separated by comma (,) or put this option
# multiple time (only on the command line, not in the configuration file where
# it should appear only once).

# C0103: Constant naming does not follow UPPERCASE convention
# C0111: Missing class docstring
# C0411: Third party import order
# E1101: Object does not have member
# R0901: Too many ancestors
# R0903: Too few public methods
# W0221: Overridden signature does not match
# W0611: Unused import
# W0613: Unused argument
# W0703: Catching too general of an exception
# W1203: Use % formatting in log messages
disable=C0103,C0111,C0411,E1101,R0901,R0903,W0221,W0611,W0613,W0703,W1203

[FORMAT]

# Maximum number of characters on a single line.
max-line-length=120